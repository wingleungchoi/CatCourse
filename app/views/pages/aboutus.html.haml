      %span.glyphicon.glyphicon-star
    %li.history-2006-large.down
      %span.glyphicon.glyphicon-star
      .box
        .dot
          %span.glyphicon.glyphicon-bookmark
        .history
          .title.year2006
            %h4 2006
          .content
            %p Integrative Nutrition leads initiatives to help ban trans fat in New York City restaurants
    %li.history-2012-large.up
      .box      
        .history
          .content
            %p The National College Credit Recommendation Service recommends us for 40 college credits
          .title.year2012
            %h4 2012
        .dot
